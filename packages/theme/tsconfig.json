{
	"compilerOptions": {
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"rootDir": ".",
		"baseUrl": ".",
		"outDir": "./dist",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Node",
		"jsx": "preserve",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"types": ["vite/client", "vitepress"],
	},
	"include": ["."]
}